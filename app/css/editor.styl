div.game-list
  .game-list-item
    td, [class^="icon-"]
      vertical-align middle     
  
div.editor
  .nav-pills
    margin 0 auto
    padding 0

  .rectangle-board
    .field
      border 1px solid gray
      width 30px
      height 30px
    .field.selected, .field.selected:hover
      box-shadow inset 0 0 3px blue
    .field:hover
      box-shadow inset 0 0 3px green
